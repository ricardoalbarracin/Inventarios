
@using inventarioImportaciones.Models;
@model inventarioImportaciones.Models.Usuarios

@{
    Html.EnableClientValidation();
    Html.EnableUnobtrusiveJavaScript();
}

<div class="container">
    <div class="row">
        <div class="text-center">
            <h1 class="pageTitle">@Resourses.Global.iniciarsesion</h1>
        </div>
        <div class="col-xs-12 col-sm-6 col-sm-offset-3 col-lg-4 col-lg-offset-4">

            <div class="col-md-12">
                <div class="col-md-4">
                    <button onclick="UpdateGridEdit(@ViewBag.GridName)" type="button" class="btn btn-orange">Aceptar</button>
                </div>

                <div class="col-md-4">
                    <button onclick="CancelGridEdit(@ViewBag.GridName)" type="button" class="btn btn-orange">Cancelar</button>
                </div>
            </div>
            
        </div>
    </div>
</div>



    